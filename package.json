{
  "name": "funfunautomator",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "start": "node src/server.js",
    "build": "npm-run-all test check audit",
    "test": "jest",
    "audit": "npm audit",
    "check": "npm-run-all check-prettier check-lint",
    "check-fix": "npm-run-all check-prettier-fix check-lint-fix",
    "check-prettier": "prettier-check src/**/*.js",
    "check-prettier-fix": "prettier --write src/**/*.js",
    "check-lint": "eslint src/**/*.js",
    "check-lint-fix": "eslint --fix src/**/*.js",
    "pre-commit": "npm test",
    "pre-push": "npm-run-all ensure-git-clean build",
    "ci-test": "jest `circleci tests glob src/**/test.js | circleci tests split` --ci --runInBand --testResultsProcessor=\"jest-junit\"",
    "ci-check": "npm run check",
    "ensure-git-clean": "[[ -z $(git status -s) ]] || echo 'Git working directory must be clean' ; exit 1"
  },
  "engines": {
    "node": "^10",
    "npm": "^6"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "eslint": "^4.18.2",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jest": "^21.14.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-react": "^7.8.2",
    "eslint-plugin-standard": "^3.0.1",
    "jest": "^22.2.2",
    "prettier": "1.11.1",
    "prettier-check": "^2.0.0"
  },
  "dependencies": {
    "@babel/core": "^7.0.0-beta.49",
    "@babel/preset-env": "^7.0.0-beta.49",
    "@babel/preset-react": "^7.0.0-beta.49",
    "apicache": "^1.2.0",
    "babel-polyfill": "^6.26.0",
    "babelify": "^9.0.0",
    "body-parser": "^1.20.1",
    "browserify": "^16.1.0",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "date-info": "^1.1.1",
    "delay": "^2.0.0",
    "express": "^4.18.2",
    "git-rev": "^0.2.1",
    "is-plain-object": "^2.0.4",
    "jest-junit": "^3.6.0",
    "node-fetch": "^2.0.0",
    "npm-run-all": "^4.1.2",
    "ramda": "^0.25.0",
    "raven": "^2.4.2",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "supersniff": "^2.1.0",
    "ws": "^4.1.0"
  }
}
